<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="mallcollection.joinearn.com.mymall">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <application
        tools:replace="android:allowBackup"
        android:allowBackup="true"
        android:name=".App.MyApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.TestTaoBaoActivity">
        </activity>
        <activity android:name=".ui.activity.AliSdkWebViewProxyActivity"></activity>
        <activity android:name=".ui.activity.AliSdkOrderActivity"></activity>
        <activity android:name=".ui.activity.AliSdkTransactionActivity"></activity>

        <activity android:name=".ui.activity.BrandDetailActivity"/>
    </application>

</manifest>